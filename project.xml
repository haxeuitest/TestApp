<?xml version="1.0" encoding="utf-8"?>
<project>
    <set name="target" value="Buttons" />
    <set name="target" value="Labels" if="labels" />
    <meta title="${target}" package="" version="1.0.0" company="" />
    <app main="${target}" file="Main" path="build/openfl-${target}" />

    <window background="#FFFFFF" fps="60" />
    <window width="800" height="600" if="desktop" />
    <window width="0" height="0" if="html5" />

    <!-- classpath, haxe libs -->
    <source path="src" />

    <haxelib name="openfl" />
<define name="dom" />
    <haxelib name="haxeui-core" />
    <haxelib name="haxeui-openfl" />
</project>
